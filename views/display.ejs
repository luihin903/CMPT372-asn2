<!DOCTYPE html>
<html lang = "en">

    <head>
        <meta charset = "UTF-8">
        <title>Recipe App</title>
        <link rel = "stylesheet" href = "display.css">
    </head>

    <body>
        <table>
            <th>Saved Recipes</th>
            <% recipes.forEach(function(recipe) { %>
                <tr><td><a href = "/recipe/<%= recipe.id %>"><%= recipe.title %></a></td></tr>
            <% }); %>
        </table>
        <div><a href = "/create"><button>CREATE</button></a></div>
    </body>

</html>